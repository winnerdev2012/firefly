    "assetDefinitionID",
    "name",
    "isContentPrivate",
    "isContentUnique"
  ],
  "properties": {
    "assetDefinitionID": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "isContentPrivate": {
      "type": "boolean"
    },
    "isContentUnique": {
      "type": "boolean"
    },
    "descriptionSchema": {
      "type": "object"
    },
    "contentSchema": {
      "type": "object"
    },
    "indexes": {
      "type": "array"
    }
  }
}